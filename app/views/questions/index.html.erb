<h3>Question</h3>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Body</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.id %></td>
        <td><%= question.body %></td>
        <td><%= link_to 'Show', test_question_path(@test, question) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
