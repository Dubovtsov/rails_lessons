<table>
  <tbody>
      <tr>
        <td><%= question.id %></td>
        <td><%= question.body %></td>
        <td class="px-1"><%= link_to admin_question_path(question) do %>
          <i class="material-icons md-18">remove_red_eye</i>
        <% end %>
        </td>
        <td class="px-1"><%= link_to edit_admin_question_path(question) do %>
          <i class="material-icons md-18">create</i>
        <% end %>
        </td>
        <td class="px-1"><%= link_to admin_question_path(question), method: :delete, data: { confirm: 'Are you sure?' } do %>
            <i class="material-icons md-18">delete</i>
          <% end %>
        </td>
      </tr>
  </tbody>
</table>
